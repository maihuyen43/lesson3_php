<?php
// Viết chương trình PHP để kiểm tra xem một số có phải là số chẵn hay không.
function parityCheck($number) {
    if($number % 2==0){
        return true;
    }else{
        return false ;
    }
}
    $number=9;
    if(parityCheck($number)){
        echo"$number là số chẵn ";
    }else{
        echo"$number không là số chẵn";
    }
    ?>